{
	"rules": {
		"no-cond-assign": [2, "always"],
		"no-console": [1],
		"no-control-regex": [2],
		"no-debugger": [1],
		"no-dupe-args": [2],
		"no-dupe-keys": [2],
		"no-duplicate-case": [2],
		"no-empty-character-class": [2],
		"no-empty": [2],
		"no-ex-assign": [2],
		"no-extra-boolean-cast": [2],
		"no-extra-parens": [2, "functions"],
		"no-extra-semi": [2],
		"no-func-assign": [2],
		"no-inner-declarations": [2, "both"],
		"no-invalid-regexp": [2],
		"no-irregular-whitespace": [2],
		"no-negated-in-lhs": [2],
		"no-obj-calls": [2],
		"no-regex-spaces": [2],
		"no-sparse-arrays": [1],
		"no-unreachable": [2],
		"use-isnan": [2],
		"valid-typeof": [2],
		"no-unexpected-multiline": [2],
		"no-undef": [2],
		"no-multi-spaces": [2],
		"no-multi-str": [1],
		"indent": [2, "tab", { "SwitchCase": 1 }],
		"quotes": [2, "single"],
		"semi": [2, "always"],
		"brace-style": [ 2, "stroustrup", { "allowSingleLine": true }],
		"no-delete-var": [2],
		"no-label-var": [2],
		"no-var": [2],
		"camelcase": [2, { "properties": "never" }],
		"consistent-return": [2],
		"curly": [2, "all"],
		"default-case": [2],
		"dot-notation": [1],
		"dot-location": [1, "property"],
		"eqeqeq": [2],
		"no-caller": [2],
		"no-case-declarations": [2],
		"no-div-regex": [1],
		"no-else-return": [1],
		"no-empty-pattern": [2],
		"no-eval": [2],
		"no-extend-native": [2],
		"no-extra-bind": [1],
		"no-eq-null": [2],
		"no-fallthrough": [2],
		"no-floating-decimal": [1],
		"no-implicit-coercion": [2, { "boolean": true, "number": true, "string": true }],
		"no-implied-eval": [2],
		"no-invalid-this": [2],
		"no-iterator": [2],
		"no-labels": [2],
		"no-lone-blocks": [1],
		"no-native-reassign": [2],
		"no-loop-func": [1],
		"no-magic-numbers": [1, { "ignore": [0, 1], "enforceConst": true }],
		"no-new-func": [2],
		"no-new-wrappers": [2],
		"no-new": [2],
		"no-octal-escape": [2],
		"no-octal": [1],
		"no-param-reassign": [2, { "props": true }],
		"no-proto": [2],
		"no-redeclare": [2],
		"no-script-url": [2],
		"no-self-compare": [2],
		"no-sequences": [2],
		"no-throw-literal": [2],
		"no-unused-expressions": [2],
		"no-useless-call": [1],
		"no-useless-concat": [1],
		"no-void": [2],
		"no-return-assign": [1, "always"],
		"no-with": [2],
		"radix": [2],
		"vars-on-top": [2],
		"wrap-iife": [2, "outside"],
		"array-bracket-spacing": [1, "never"],
		"block-spacing": [1, "always"],
		"comma-dangle": [1, "never"],
		"comma-spacing": [2, { "before": false, "after": true }],
		"comma-style": [1, "last"],
		"computed-property-spacing": [1, "never"],
		"eol-last": [2],
		"key-spacing": [2, { "beforeColon": false, "afterColon": true }],
		"new-cap": [2],
		"new-parens": [2],
		"no-array-constructor": [2],
		"no-inline-comments": [1],
		"no-lonely-if": [2],
		"no-mixed-spaces-and-tabs": [2],
		"no-multiple-empty-lines": [1, { "max": 1 }],
		"no-nested-ternary": [2],
		"no-new-object": [2],
		"no-plusplus": [2, { "allowForLoopAfterthoughts": true }],
		"no-spaced-func": [1],
		"no-trailing-spaces": [1, { "skipBlankLines": false }],
		"no-underscore-dangle": [1],
		"no-unneeded-ternary": [1],
		"object-curly-spacing": [2, "always", { "objectsInObjects": true }],
		"one-var": [2, "never"],
		"operator-linebreak": [1, "before", { "overrides": { "=": "after" } }],
		"padded-blocks": [1, "never"],
		"semi-spacing": [2, {"before": false, "after": true}],
		"keyword-spacing": [2, {"before": true, "after": true, "overrides": {}}],
		"space-before-blocks": [1, "always"],
		"space-before-function-paren": [1, "always"],
		"space-in-parens": [1, "never"],
		"space-infix-ops": [1],
		"space-unary-ops": [1, { "words": true, "nonwords": false }],
		"spaced-comment": [1, "always", { "exceptions": ["/"] }],
		"wrap-regex": [1],
		"arrow-parens": [2, "always"],
		"arrow-spacing": [2, { "before": true, "after": true } ],
		"no-constant-condition": [2],
		"no-const-assign": [2],
		"object-shorthand": [1, "always"],
		"prefer-arrow-callback": [1],
		"prefer-const": [1],
		"prefer-template": [1],
		"prefer-spread": [1],
		"no-shadow-restricted-names": [2],
		"no-shadow": [2, { "builtinGlobals": true, "hoist": "all", "allow": ["find", "test"] }],
		"no-unused-vars": [2, {"vars": "all", "args": "all"}],
		"no-use-before-define": [2],
		"callback-return": [2],
		"global-require": [2],
		"handle-callback-err": [2]
	},
	"env": {
		"es6": true,
		"node": true,
		"mocha": true,
		"browser": true
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	}
}